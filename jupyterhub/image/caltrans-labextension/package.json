{
  "name": "caltrans-labextension",
  "version": "0.1.0",
  "description": "A JupyterLab extension for caltrans / cal-itp customizations",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/cal-itp/data-infra",
  "bugs": {
    "url": "https://github.com/cal-itp/data-infra/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Hunter Owens, adapted from Ian Rose",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "welcome.md"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
